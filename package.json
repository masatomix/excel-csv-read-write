{
  "name": "ts-node-template",
  "version": "0.1.0-SNAPSHOT",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "lint": "tslint --fix --project tsconfig.json",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "start": "node .",
    "test": "NODE_ENV= jest"
  },
  "keywords": [],
  "author": "Masatomi KINO <masatomix@ki-no.org> (http://qiita.com/masatomix)",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.spec.+(ts|tsx|js)"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/config": "0.0.36",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.1",
    "@types/unzip": "^0.1.1",
    "jest": "^25.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "config": "^3.2.5",
    "bunyan": "^1.8.12",
    "csvtojson": "^2.0.10",
    "iconv-lite": "^0.5.1",
    "unzip": "^0.1.11",
    "xlsx-populate": "^1.21.0"
  }
}
